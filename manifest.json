{
  "manifest_version": 2,
  "name": "PhishNet",
  "version": "1.0",
  "description": "Track and toggle phishing protection",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "PhishNET.png"
  },
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "icons": {
  },
  "permissions": [
    "storage","tabs","webNavigation"
  ]
}